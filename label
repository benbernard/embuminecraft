args = {...}

sorter = peripheral.wrap("bottom")
list = sorter.list(5)
itemToLabel = 0
for item,count in pairs(list) do
   itemToLabel = item
end 
print("found item " .. itemToLabel)

geturl = "http://www.technofetish.net/minecraft.txt"
labels = textutils.unserialize(http.get(geturl))
for i,str in pairs(args) do
   labels[str] = itemToLabel
end
posturl = "http://www.technofetish.net/cgi-bin/minecraft_save.cgi"
serialized = textutils.serialize(labels)
http.post(posturl, serialized)
print("posted!")